package com.hengyu.common.entity;

public class AccessToken {

    /**
     * 获取到的凭证
     */
    private String token;
    
    /**
     * 凭证有效时间，单位：秒
     */
    private int expiresIn;
    
    public AccessToken(String token, int expiresIn)
    {
        super();
        this.token = token;
        this.expiresIn = expiresIn;
    }
    
    public AccessToken()
    {
        super();
    }
}
