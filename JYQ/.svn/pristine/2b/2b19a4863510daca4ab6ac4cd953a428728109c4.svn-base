package com.hengyu.cases.service.impl;

import com.hengyu.cases.entity.Case;
import com.hengyu.cases.dao.CaseDAO;
import com.hengyu.cases.service.CaseService;
import com.hengyu.cases.vo.CaseInfoVo;
import com.hengyu.common.msg.RestResponse;
import com.baomidou.mybatisplus.service.impl.ServiceImpl;

import java.util.List;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

/**
 * <p>
 * 中介案例表 服务实现类
 * </p>
 *
 * @author allnas
 * @since 2018-08-28
 */
@Service
public class CaseServiceImpl extends ServiceImpl<CaseDAO, Case> implements CaseService {
	@Autowired
	CaseDAO caseDao;
	
	@Override
	public CaseInfoVo queryCaseInfoById(Integer caseId) {
		// TODO Auto-generated method stub
		return caseDao.queryCaseInfoById(caseId);
	}

	@Override
	public List<CaseInfoVo> queryCaseInfoByCate(Map<String, Object> map) {
		// TODO Auto-generated method stub
		return caseDao.queryCaseInfoByCate(map);
	}

	
	

	

}
