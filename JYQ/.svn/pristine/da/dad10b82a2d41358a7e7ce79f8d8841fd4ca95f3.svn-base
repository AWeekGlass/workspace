<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hengyu.user.dao.OrganizationDAO">
	<select id="queryAllByResourceId" resultType="OrganizationVo">
		SELECT `name`,`value` FROM sys_organization WHERE resource_id = #{resourceId}
	</select>
</mapper>
