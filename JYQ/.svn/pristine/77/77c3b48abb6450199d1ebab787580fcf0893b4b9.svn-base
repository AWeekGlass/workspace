package com.hengyu.cases.service.impl;

import com.hengyu.cases.entity.Comment;
import com.hengyu.cases.dao.CommentDAO;
import com.hengyu.cases.service.CommentService;
import com.hengyu.cases.vo.CommentInfoVo;
import com.baomidou.mybatisplus.service.impl.ServiceImpl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

/**
 * <p>
 *  服务实现类
 * </p>
 *
 * @author allnas
 * @since 2018-08-28
 */
@Service
public class CommentServiceImpl extends ServiceImpl<CommentDAO, Comment> implements CommentService {
	@Autowired
	CommentDAO commentDao;

	@Override
	public List<CommentInfoVo> queryCommentInfoByCaseId(Integer caseId) {
		return commentDao.queryCommentInfoByCaseId(caseId);
	}

}
